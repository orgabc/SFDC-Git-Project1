    <validationRules>
        <fullName>Close_Date_Rule</fullName>
        <active>true</active>
        <description>Makes sure that the status is not Closed if the closed date is blank</description>
        <errorConditionFormula>AND(  ISBLANK(Close_Date__c ),  
     OR( ISPICKVAL( Status__c , &quot;Closed-Filled&quot;),  
         ISPICKVAL( Status__c , &quot;Closed-Approved&quot;),
         ISPICKVAL( Status__c , &quot;Closed-Cancelled&quot;)  
       )
     )</errorConditionFormula>
        <errorDisplayField>Close_Date__c</errorDisplayField>
        <errorMessage>Closed Date must be specified if status is closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MinPay_Rule</fullName>
        <active>true</active>
        <description>Min Pay should never be grater than Max Pay</description>
        <errorConditionFormula>Min_Pay__c  &gt;  Max_Pay__c</errorConditionFormula>
        <errorMessage>Minimum pay cannot exceed maximum pay value</errorMessage>
    </validationRules>
